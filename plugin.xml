<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-root-detector-v2" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>RootDetectorv2</name>
    <js-module name="RootDetectorv2" src="www/RootDetectorv2.js">
        <clobbers target="rootdetector"/>
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="RootDetectorv2">
                <param name="android-package" value="cordova-plugin-root-detector-v2.RootDetectorv2"/>
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/RootDetectorv2.java"
                     target-dir="src/android/RootDetectorv2"/>
    </platform>
</plugin>